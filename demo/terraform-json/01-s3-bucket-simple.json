{
  "name": "demo-s3-bucket-simple",
  "terraform_json": {
    "terraform": {
      "required_providers": {
        "random": {
          "source": "hashicorp/random",
          "version": "3.6.0"
        },
        "aws": {
          "source": "hashicorp/aws",
          "version": "6.2.0"
        }
      }
    },
    
    "provider": {
      "aws": {
        "profile": "developer",
        "region": "us-east-1"
      }
    },
    
    "resource": {
      "random_string": {
        "bucket_suffix": {
          "length": 8,
          "special": false,
          "upper": false
        }
      },
      "aws_s3_bucket": {
        "demo": {
          "bucket": "lattiam-simple-bucket-${random_string.bucket_suffix.result}",
          "timeouts": {}
        }
      }
    }
  },
  "config": {
    "aws_region": "us-east-1",
    "aws_profile": "developer"
  }
}